{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src",
    "paths": {
      "@data/*": ["data/*"],
      "@types/*": ["types/*"],
      "@components/*": ["components/*"],
      "@items/*": ["items/*"]
    },
    "typeRoots": [".src/types"]
  }
}