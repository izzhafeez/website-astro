---
import NavBar from "../../../components/common/NavBar.astro";
import Seo from "../../../components/seo/Seo.astro";
import seoImg from '/src/img/heroes/FREQUENCY-GUESSR.jpg';
import gamesData from "../../../data/games/games.json";
import Layout from "../../../layouts/Layout.astro";
import FrequencyGuessrRoomCreate from "../../../components/games/frequency-guessr/FrequencyGuessrRoomCreate";
import PageFind from "../../../components/common/PageFind.astro";
const heroText = gamesData["frequency-guessr"].heroText;
---

<Seo {heroText} {seoImg} pageName={"FrequencyGuessr"} path={`games/frequency-guessr`}/>

<Layout>
  <div slot="main" class="min-h-screen h-auto flex flex-col">
    <NavBar/>
    <PageFind pageName={"FrequencyGuessr"} heroText={heroText}/>
    <FrequencyGuessrRoomCreate client:only="react"/>
  </div>
</Layout>