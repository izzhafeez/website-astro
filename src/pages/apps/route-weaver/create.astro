---
import NavBar from "../../../components/common/NavBar.astro";
import RouteWeaver from "../../../components/apps/route-weaver/RouteWeaver.vue";
import Layout from "../../../layouts/Layout.astro";
import appsData from "../../../data/apps/apps.json";
import Seo from "../../../components/seo/Seo.astro";
import seoImg from "../../../img/heroes/ROUTE-WEAVER.jpg";

export const prerender = true;
const heroText = appsData["route-weaver"].heroText;
---
<Seo {heroText} {seoImg} pageName={"RouteWeaver"} path="apps/route-weaver"/>

<Layout>
  <div slot="main">
    <NavBar/>
    <h1 class="hidden" data-pagefind-weight="10.0" data-pagefind-meta="title">RouteWeaver (Create Route)</h1>
    <RouteWeaver client:only="vue"/>
  </div>
</Layout>