---
import NavBar from "../../../components/common/NavBar.astro";
import CreateForm from "../../../components/meetupmaker/CreateForm.vue";
import Layout from "../../../layouts/Layout.astro";

export const prerender = true;
---

<Layout>
  <div slot="main">
    <NavBar/>
    <h1 class="my-8 text-6xl max-w-xl mx-2 sm:mx-auto font-extrabold bg-gradient-to-r from-ew-500 to-ew-300 dark:from-ew-500 dark:to-ew-100 text-transparent bg-clip-text">
      Create Meetup
    </h1>
    <CreateForm client:only="vue"/>
  </div>
</Layout>
<link href="https://cdn.jsdelivr.net/npm/@sweetalert2/theme-minimal@4/minimal.css" rel="stylesheet">
<script is:inline src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.js"></script>