---
import { allItems } from '../../../../items/getItems';
import Layout from '../../../../layouts/Layout.astro';

export const getStaticPaths = (() => {
  return allItems.map(item => ({
    params: {
      category: item.category,
      type: item.type,
      key: item.key,
    },
    props: { item }
  }))
});

export const prerender = true;
